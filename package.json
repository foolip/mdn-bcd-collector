{
  "name": "mdn-bcd-collector",
  "description": "Data collection service for mdn/browser-compat-data",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/foolip/mdn-bcd-collector.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "install": "cd node_modules/puppeteer && PUPPETEER_PRODUCT=firefox node install.js",
    "lint": "eslint .",
    "build": "node build.js",
    "gcp-build": "npm run build",
    "deploy": "gcloud app deploy",
    "start": "node app.js",
    "coverage": "NODE_ENV=test nyc mocha --recursive test --exclude test/puppeteer/*",
    "puppeteer": "NODE_ENV=test mocha test/puppeteer --timeout 30000 --slow 3000",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "npm run coverage && npm run puppeteer"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "4.0.0",
    "@google-cloud/storage": "5.1.2",
    "@octokit/rest": "18.0.1",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "fs-extra": "9.0.1",
    "json-stable-stringify": "1.0.1",
    "json3": "3.3.3",
    "mdn-browser-compat-data": "1.0.31",
    "reffy-reports": "git://github.com/foolip/reffy-reports.git#mdn-bcd-updater-fixes",
    "slugify": "1.4.4",
    "ua-parser-js": "0.7.21",
    "unique-string": "2.0.0",
    "webidl2": "23.12.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "chai-subset": "1.6.0",
    "codecov": "3.7.1",
    "eslint": "7.5.0",
    "eslint-config-google": "0.14.0",
    "mocha": "8.0.1",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "puppeteer": "5.2.0",
    "sinon": "9.0.2"
  }
}
