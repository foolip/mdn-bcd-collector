{
  "name": "mdn-bcd-collector",
  "description": "Data collection service for mdn/browser-compat-data",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/foolip/mdn-bcd-collector.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepare": "if [ -d node_modules/puppeteer ]; then cd node_modules/puppeteer && PUPPETEER_PRODUCT=firefox node install.js; fi",
    "lint": "eslint . && ejslint views/*",
    "lint-fix": "eslint --fix .",
    "clean": "rm -rf .nyc_output coverage generated tests.json",
    "build": "node build.js",
    "gcp-build": "npm run build",
    "deploy": "gcloud app deploy",
    "start": "node app.js",
    "start-dev": "nodemon app.js",
    "unittest": "NODE_ENV=test nyc mocha --recursive unittest",
    "coverage": "nyc report -r lcov && open-cli coverage/lcov-report/index.html",
    "test": "npm run lint && npm run unittest",
    "selenium": "node selenium.js",
    "update-bcd": "node update-bcd.js",
    "find-missing": "node find-missing.js"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "4.0.0",
    "@google-cloud/storage": "5.3.0",
    "@octokit/rest": "18.0.5",
    "cookie-parser": "1.4.5",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "express-ejs-layouts": "2.5.0",
    "json-stable-stringify": "1.0.1",
    "slugify": "1.4.5",
    "ua-parser-js": "0.7.21",
    "unique-string": "2.0.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "chai-fs": "2.0.0",
    "chai-http": "4.3.0",
    "chai-subset": "1.6.0",
    "compare-versions": "3.6.0",
    "ejs-lint": "1.1.0",
    "eslint": "7.8.1",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "fs-extra": "9.0.1",
    "json3": "3.3.3",
    "mdn-browser-compat-data": "1.0.36",
    "mocha": "8.1.3",
    "mock-fs": "4.13.0",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "open-cli": "6.0.1",
    "proxyquire": "2.1.3",
    "puppeteer": "5.2.1",
    "puppeteer-to-istanbul": "1.4.0",
    "selenium-webdriver": "3.6.0",
    "sinon": "9.0.3",
    "webidl2": "23.13.0",
    "webref": "git://github.com/foolip/webref.git#mdn-bcd-updater-fixes"
  }
}
