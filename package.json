{
  "name": "mdn-bcd-collector",
  "description": "Data collection service for @mdn/browser-compat-data",
  "version": "3.1.2",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/foolip/mdn-bcd-collector.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "node scripts.js prepare",
    "lint": "eslint . && prettier -c \"**/*.json\" \"**/*.md\" \"**/*.yaml\" --color && ejslint views/*",
    "lint-fix": "eslint --fix . && prettier --write \"**/*.json\" \"**/*.md\" \"**/*.yaml\"",
    "clean": "rm -rf .nyc_output coverage generated tests.json",
    "build": "node build.js",
    "gcp-build": "npm run build",
    "deploy": "gcloud app deploy",
    "start": "node app.js",
    "start-dev": "npm i && nodemon -e js,json,idl --exec \"npm run build && npm start\"",
    "unittest": "node scripts.js unittest",
    "coverage": "nyc report -r lcov && open-cli coverage/lcov-report/index.html",
    "test": "npm run lint && npm run unittest",
    "selenium": "node selenium.js",
    "update-bcd": "node update-bcd.js",
    "find-missing": "node find-missing.js",
    "add-new-bcd": "node add-new-bcd.js"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "4.0.5",
    "@google-cloud/storage": "5.8.5",
    "@mdi/font": "5.9.55",
    "@mdn/browser-compat-data": "3.3.4",
    "@octokit/rest": "18.5.4",
    "compare-versions": "3.6.0",
    "cookie-parser": "1.4.5",
    "didyoumean": "1.2.1",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "express-ejs-layouts": "2.5.0",
    "json-stable-stringify": "1.0.1",
    "slugify": "1.5.3",
    "ua-parser-js": "0.7.28",
    "unique-string": "2.0.0",
    "winston": "3.3.3",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@browser-logos/chrome": "1.0.18",
    "@browser-logos/edge": "2.0.5",
    "@browser-logos/firefox": "3.0.9",
    "@browser-logos/internet-explorer_9-11": "1.1.15",
    "@browser-logos/opera": "1.1.11",
    "@browser-logos/safari": "2.0.0",
    "@webref/css": "1.0.2",
    "@webref/idl": "2.2.0",
    "babel-eslint": "10.1.0",
    "chai": "4.3.4",
    "chai-fs": "2.0.0",
    "chai-http": "4.3.0",
    "chai-subset": "1.6.0",
    "chalk": "4.1.1",
    "ejs-lint": "1.2.0",
    "eslint": "7.27.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "fs-extra": "10.0.0",
    "json3": "3.3.3",
    "klaw": "3.0.0",
    "listr2": "3.9.0",
    "mocha": "8.4.0",
    "node-fetch": "2.6.1",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "open-cli": "6.0.1",
    "prettier": "2.3.0",
    "proxyquire": "2.1.3",
    "puppeteer": "9.1.1",
    "puppeteer-to-istanbul": "1.4.0",
    "selenium-webdriver": "3.6.0",
    "sinon": "11.1.1",
    "webidl2": "24.1.1"
  }
}
