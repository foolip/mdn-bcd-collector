{
  "name": "mdn-bcd-collector",
  "description": "Data collection service for @mdn/browser-compat-data",
  "version": "6.2.11",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/foolip/mdn-bcd-collector.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "format": "eslint . && prettier -c \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\" --color && ejslint views/*",
    "format:fix": "eslint --fix . && prettier --write \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\"",
    "fix": "npm run format:fix",
    "clean": "rm -rf .nyc_output coverage generated tests.json",
    "build": "npm run build:compile && npm run build:tests && npm run build:resources",
    "build:compile": "tsc -p .",
    "build:tests": "ts-node build.ts",
    "build:resources": "ts-node prepare-resources.ts",
    "gcp-build": "npm run build",
    "deploy": "gcloud app deploy",
    "start": "NODE_ENV=production node app.js",
    "dev": "npm i && nodemon -e ts,json,idl,scss,yaml --exec \"npm run build:tests && npm run build:resources && ts-node app.ts\"",
    "unittest": "NODE_ENV=test c8 mocha --recursive 'unittest/**/*.ts'",
    "coverage": "npm run coverage:report && npm run coverage:open",
    "coverage:report": "c8 report -r lcov",
    "coverage:open": "open-cli coverage/lcov-report/index.html",
    "test": "npm run format && npm run unittest",
    "selenium": "ts-node selenium.ts",
    "update-bcd": "ts-node update-bcd.ts",
    "find-missing-features": "ts-node find-missing-features.ts",
    "find-missing-results": "ts-node find-missing-results.ts",
    "add-new-bcd": "ts-node add-new-bcd.ts",
    "release": "ts-node release.ts",
    "result-stats": "ts-node result-stats.ts"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "5.3.0",
    "@google-cloud/storage": "6.9.4",
    "@mdi/font": "7.1.96",
    "@mdn/browser-compat-data": "5.2.31",
    "@octokit/rest": "19.0.5",
    "compare-versions": "5.0.3",
    "cookie-parser": "1.4.6",
    "didyoumean": "1.2.2",
    "ejs": "3.1.8",
    "es-main": "1.2.0",
    "express": "4.18.2",
    "express-csp-header": "5.1.0",
    "express-ejs-layouts": "2.5.1",
    "fs-extra": "11.1.0",
    "json-stable-stringify": "1.0.1",
    "slugify": "1.6.5",
    "ua-parser-js": "1.0.33",
    "unique-string": "3.0.0",
    "winston": "3.8.2",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-syntax-import-assertions": "7.20.0",
    "@babel/preset-env": "7.20.2",
    "@browser-logos/chrome": "2.0.0",
    "@browser-logos/edge": "2.0.5",
    "@browser-logos/firefox": "3.0.9",
    "@browser-logos/internet-explorer_9-11": "1.1.16",
    "@browser-logos/opera": "1.1.11",
    "@browser-logos/safari": "2.1.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.36",
    "@types/minimatch": "5.1.2",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.18",
    "@types/superagent": "4.1.16",
    "@types/webidl2": "23.13.6",
    "@types/yargs": "17.0.20",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@webref/css": "6.4.2",
    "@webref/idl": "3.24.0",
    "c8": "7.12.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "chai-fs": "2.0.0",
    "chai-http": "4.3.0",
    "chai-subset": "1.6.0",
    "chalk": "5.2.0",
    "chalk-template": "0.5.0",
    "ejs-lint": "2.0.0",
    "enquirer": "2.3.6",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "45.0.2",
    "fdir": "5.3.0",
    "json3": "3.3.3",
    "klaw": "4.1.0",
    "listr2": "5.0.7",
    "minimatch": "7.4.2",
    "mocha": "10.2.0",
    "node-fetch": "3.3.0",
    "nodemon": "2.0.20",
    "open-cli": "7.1.0",
    "prettier": "2.8.3",
    "puppeteer": "19.5.2",
    "sass": "1.57.1",
    "selenium-webdriver": "4.8.1",
    "sinon": "15.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "webidl2": "24.2.2",
    "yaml": "2.2.1"
  }
}
